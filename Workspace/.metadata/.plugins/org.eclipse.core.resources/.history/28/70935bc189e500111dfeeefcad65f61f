AjaxRoute = require('../../../AjaxRoute')
AjaxResponse = require('../../../../serialization/AjaxResponse')
UserAuth = require('../../../../security/UserAuth')
class ListAircraftManufacturersRoute extends AjaxRoute
	constructor: () ->
		super [{ method: 'POST', path: '/refreshToken' }, { method: 'GET', path: '/refreshToken' }]
	get: (req, res) ->
	post: (req, res) ->
		resp = new AjaxResponse(null)
		resp.failure('Method Not Allowed', 405)
		res.json(resp, 200)
	isValidRequest: (req) ->
		if req.query? and req.query?.token?
			true
		else
			false 
	
module.exports = new RefreshTokenRoute()