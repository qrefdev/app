class RpcRequest
	###
	@property [String] (Required) The type of request. Should always be "rpc" for URLs beginning with '/services/rpc/' prefix.
	###
	mode: "rpc"
	###
	@property [String] (Optional) The authentication token to include the request.
	###
	token: null
	constructor: (token) -> 
		@mode = "rpc"
		@token = token
	 