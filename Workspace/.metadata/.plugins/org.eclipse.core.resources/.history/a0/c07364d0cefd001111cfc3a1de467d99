mongoose = require('mongoose')
Schema = mongoose.Schema
ObjectId = Schema.ObjectId

###
@author Nathan Klick
@copyright QRef 2012
@abstract
###
class RoleSchemaInternal 
	roleName:
		type: String
		required: true
		unique: true
	description:
		type: String
		required: false

RoleSchema = new Schema(new RoleSchemaInternal())
module.exports = RoleSchema